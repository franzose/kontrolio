{
  "name": "franzose/kontrolio",
  "type": "library",
  "description": "Simple and standalone data validation library inspired by Laravel and Symfony",
  "keywords": ["validation", "laravel", "symfony"],
  "license": "MIT",
  "authors": [
    {
      "name": "Jan Iwanow",
      "email": "iwanow.jan@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5|^10|^11|^12"
  },
  "autoload": {
    "psr-4": {
      "Kontrolio\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kontrolio\\Tests\\": "tests/"
    }
  }
}
